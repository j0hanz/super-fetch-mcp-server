{
  "name": "@j0hanz/superfetch",
  "version": "1.0.2",
  "mcpName": "io.github.j0hanz/superfetch",
  "description": "Intelligent web content fetcher MCP server that converts HTML to clean, AI-readable JSONL format",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "superfetch": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j0hanz/super-fetch-mcp-server.git"
  },
  "homepage": "https://github.com/j0hanz/super-fetch-mcp-server#readme",
  "bugs": {
    "url": "https://github.com/j0hanz/super-fetch-mcp-server/issues"
  },
  "author": "j0hanz",
  "license": "MIT",
  "keywords": [
    "mcp",
    "mcp-server",
    "web-fetching",
    "content-extraction",
    "readability",
    "jsonl",
    "ai-tools"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run lint",
    "start": "node dist/index.js",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "test": "echo \"Tests not yet implemented - see Phase 4 roadmap\" && exit 0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@mozilla/readability": "^0.6.0",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "domhandler": "^5.0.3",
    "express": "^4.21.2",
    "jsdom": "^25.0.1",
    "node-cache": "^5.1.2",
    "turndown": "^7.2.0",
    "winston": "^3.19.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.10.2",
    "@types/turndown": "^5.0.5",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "knip": "^5.72.0",
    "prettier": "^3.7.4",
    "shx": "^0.3.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
